net_adm:ping('contact1@linux-mp').
Vsn = "0.1-SNAPSHOT".
File = "/home/ghaskins/sandbox/git/edist/demo/client/release/target/client-release-0.1-SNAPSHOT.tar.gz".
ok = controller:create_release("client", Vsn, "-yaws embedded true", []).
{ok, ODev} = controller:upload_release("client", Vsn, matchall, []).
{ok, IDev} = file:open(File, [read, binary]).
file:copy(IDev, ODev).
file:close(IDev).
controller:close_stream(ODev).
ok = controller:commit_release("client", Vsn, []).

