application:start(sasl).
application:start(mnesia).
application:start(controller).
net_adm:ping('contact1@linux-mp').
Vsn = "1.0.1-SNAPSHOT".
File = "/home/ghaskins/sandbox/git/edist/example-" ++ Vsn ++ ".tar.gz".
ok = controller:create_release("example", Vsn, "-yaws embedded true", []).
{ok, ODev} = controller:upload_release("example", "1.0.1-SNAPSHOT", [], []).
{ok, IDev} = file:open(File, [read, binary]).
file:copy(IDev, ODev).
file:close(IDev).
controller:close_stream(ODev).

f(IDev).
f(ODev).

{ok, IDev} = controller:download_release("example").
{ok, ODev} = file:open("/tmp/foo", [write, binary]).
file:copy(IDev, ODev).
controller:close_stream(IDev).
file:close(ODev).

